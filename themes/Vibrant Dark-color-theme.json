{
	"name": "Vibrant Dark",
	"type": "dark",
	"colors": {
		"activityBar.background": "#1A1C25",
		"activityBarBadge.background": "#9634cb",
		"debugToolBar.background": "#090B10",
		"editor.background": "#0F111A",
		"editorHoverWidget.background": "#0F111A",
		"editorGroupHeader.tabsBackground": "#1A1C25",
		"notifications.background": "#090B10",
		"sideBar.background": "#181A1F",
		"tab.inactiveBackground": "#191A21",
		"titleBar.activeBackground": "#090B10",
		"dropdown.background": "#090B10"
	},
	"tokenColors": [
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#ae55de"
			}
		},
		{
			"name": "Control Keyword",
			"scope": [
				"keyword.control"
			],
			"settings": {
				"foreground": "#be79e5"
			}
		},
		{
			"name": "Operator Keyword",
			"scope": [
				"keyword.operator",
				"constant",
				"variable.language"
			],
			"settings": {
				"foreground": "#cf9aeb"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation.separator",
				"punctuation.terminator",
				"punctuation.accessor"
			],
			"settings": {
				"foreground": "#e3c6f3"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable",
				"entity.name.import",
				"entity.name.package",
				"storage.modifier.import",
				"storage.modifier.package"
			],
			"settings": {
				"foreground": "#f1f1b0"
			}
		},
		{
			"name": "Function",
			"scope": [
				"entity.name.function",
				"variable.function",
				"support.function"
			],
			"settings": {
				"foreground": "#d7d726"
			}
		},
		{
			"name": "Type",
			"scope": [
				"storage.type",
				"entity.other",
				"entity.name"
			],
			"settings": {
				"foreground": "#d79d26",
			}
		},
		{
			"name": "Line Comment",
			"scope": [
				"comment.line"
			],
			"settings": {
				"foreground": "#58a558"
			}
		},
		{
			"name": "Block Comment",
			"scope": [
				"comment.block"
			],
			"settings": {
				"foreground": "#4cb14c"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
			],
			"settings": {
				"foreground": "#b1f0b1"
			}
		},
		{
			"name": "Escape Characters",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#156e15"
			}
		},
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		}
	]
}
